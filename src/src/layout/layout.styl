body
	background-color white
  user-select none
	animation pulse 0.2s no-repeat

@keyframes fadeIn {
	0% {
		opacity: 0;
	}
	100% {
		opacity: 1;
	}
}

@keyframes fadeOut{
	0% {
		opacity: 1;
	}
	100% {
		opacity: 0;
	}
}

.example-enter
	opacity 0.01
  transition opacity .5s ease-in


.example-enter.example-enter-active
  opacity 1


.example-leave
  opacity 1
  transition opacity .5s ease-in


.example-leave.example-leave-active
  opacity 0.01


